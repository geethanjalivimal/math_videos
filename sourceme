# Sourceme file

if [ "$(expr substr $(uname -s) 1 10)" == "MINGW64_NT" ]; then
    export CARS_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd -W)"
    export PYTHONPATH="${PYTHONPATH};${CARS_DIR}"
else
    export CARS_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
    export PYTHONPATH=${PYTHONPATH}:${CARS_DIR}
fi

